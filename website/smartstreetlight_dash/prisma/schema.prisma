generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  published Boolean @default(false)
  authorId  Int?
  user      User?   @relation(fields: [authorId], references: [id])
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  post  Post[]
}

model StreetLightData {
  id    Int     @id @default(autoincrement())
  date  DateTime
  energyUsage Int 
  lightStatus String
  brightnessLevel Int 
  powerConsumption Int
  batteryStatus Int
  sensorHealth String
  location String

}