// generates prismaclient for js 
generator client {
  provider = "prisma-client-js"
}

// specify datasource 
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LangaraData {
  id                Int      @id @default(autoincrement())
  reading_time      DateTime
  energy_usage      Int
  light_status      String
  brightness_level  Int
  power_consumption Int
  battery_status    Int
  sensor_health     String
  location          String
}

